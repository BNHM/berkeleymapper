<?xml version="1.0" encoding="UTF-8"?>
<bnhmmaps>
<metadata>
 <name>VertNet Map Configuration File</name>
 <relatedinformation>http://vertnet.org/</relatedinformation>
 <abstract>BerkeleyMapper configuration file for VertNet portal </abstract>
 <mapkeyword keyword="occurrences"/>
 <turnoffiframe iframe="1"></turnoffiframe>
 <header location="http://68.111.46.217/berkeleymapper/images/vertnet_logo_web.jpg"/>
 <footer location="http://68.111.46.217/berkeleymapper/footers/vertnetfooter.html"/>
 <linkbackheader location="http://68.111.46.217/pres/PresentationServlet?action=home"/>
 <charset name="UTF-8"></charset>
 <units key="km"></units>
<!--
 <extent/>
 <minscale value="200"></minscale>
 <maxscale value="5000"></maxscale>
-->
</metadata>

<settings>
 <setting name="landsat" show="1"/>
 <setting name="globe_bw" show="0"/>
 <setting name="drg" show="1"/>
 <setting name="doq" show="1"/>
 <setting name="finerelief" show="1"/>
 <setting name="slowcxn" show="1"/>
 <setting name="maxerrorinmeters" show="1"/>
 <setting name="colorprecision" show="1"/>
 <setting name="coloryearcollected" show="1"/>
 <setting name="label" show="1"/>
 <setting name="labelby" show="1"/>
 <setting name="opaque" show="1"/>
 <setting name="units" show="1"/>
 <setting name="backgroundimage" show="1"/>
 <setting name="embedscalebar" show="1"/>
</settings>

<recordlinkback>
 <linkback method="entireurl" linkurl="RelatedInformation" fieldname="Link Back"/>
</recordlinkback>

<themes>
 <theme method="coloryearcollected" fieldname="darwin:yearcollected"/>
 <theme method="colorprecision" fieldname="darwin:coordinateuncertaintyinmeters"/>
</themes>

<colors method="dynamicfield" fieldname="char120_1" label="Institution">
 <dominantcolor     webcolor="339933"/>
 <subdominantcolor  webcolor="eeeeee"/>
 <textdominantcolor webcolor="ffffff"/>
 <rowselectcolor    webcolor="ffff00"/>
</colors>

<concepts xmlns:darwin="http://digir.net/schema/conceptual/darwin/2003/1.0">
 <concept order="1"  viewlist="1" datatype="char120_1" alias="Institution"/>
 <concept order="21" viewlist="0" datatype="darwin:collectioncode" alias="Collection"/>
 <concept order="2"  viewlist="1" datatype="darwin:catalognumbertext" alias="Catalog Number"/>
 <concept order="20" viewlist="1" datatype="darwin:datelastmodified" alias="Date Last Modified"/>
 <concept order="5"  viewlist="1" datatype="darwin:scientificname" alias="Scientific Name"/>
 <concept order="12" viewlist="1" datatype="darwin:decimallatitude" alias="DecimalLatitude"/>
 <concept order="13" viewlist="1" datatype="darwin:decimallongitude" alias="DecimalLongitude"/>
 <concept order="14" viewlist="1" datatype="darwin:coordinateuncertaintyinmeters" alias="Coordinate Uncertainty (m)"/>
 <concept order="15" viewlist="1" datatype="darwin:horizontaldatum" alias="Horizontal Datum"/>
 <concept order="6"  viewlist="1" datatype="darwin:country" alias="Country"/>
 <concept order="7"  viewlist="1" datatype="darwin:stateprovince" alias="State Province"/> 
 <concept order="8"  viewlist="1" datatype="darwin:county" alias="County"/>
 <concept order="9"  viewlist="1" datatype="char255_1" alias="Island"/>
 <concept order="10" viewlist="1" datatype="char255_2" alias="Island Group"/>
 <concept order="11" viewlist="1" datatype="darwin:locality" alias="Locality"/>
 <concept order="16" viewlist="1" datatype="darwin:verbatimelevation" alias="Verbatim Elevation"/>
 <concept order="4"  viewlist="1" datatype="darwin:yearcollected" alias="Year Collected"/>
 <concept order="3"  viewlist="1" datatype="darwin:collector" alias="Collector"/>
 <concept order="17" viewlist="1" datatype="darwin:georefmethod" alias="Georef Method"/>
 <concept order="18" viewlist="1" datatype="darwin:latlongcomments" alias="LatLong Comments"/>
 <concept order="19" viewlist="1" datatype="darwin:relatedinformation" alias="Related Information"/>
 <concept order="22" viewlist="0" datatype="darwin:basisofrecord" alias="Basis Of Record"/>
</concepts>

<annotation show="1">
 <annotation_replyto_email value="tuco@berkeley.edu"/>
</annotation>

</bnhmmaps>