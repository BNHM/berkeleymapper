<?xml version="1.0" encoding="UTF-8"?>
<berkeleymapper>
    <metadata>
        <name>AmphibiaWeb Configuration File</name>
        <relatedinformation>http://amphibiaweb.org/</relatedinformation>
        <abstract>GIS configuration file for AmphibiaWeb</abstract>
        <mapkeyword keyword="specimens"/>
        <header location="/usr/local/web/aw/include/bm_header.html"/>
        <footer location="/usr/local/web/aw/include/bm_footer.html"/>
        <linkbackheader location=" http://amphibiaweb.org/"/>
        <legendText><![CDATA[Important information to <i>display</i> for user]]></legendText>
    </metadata>
    <gisdata>
        <layer title="IUCN Anaxyrus canorus Distribution" legend="1" active="1"
               url="http://www.iucnredlist.org/apps/redlist/details/3180/0">
            <![CDATA[http://amphibiaweb.org/cgi/amphib_ws_shapefile?format=kmz&genus=Anaxyrus&species=canorus]]></layer>
    </gisdata>
    <logo img="http://amphibiaweb.org/images/redlist_logo.jpg" url="http://www.iucnredlist.org/initiatives/amphibians"/>
    <logo img="http://amphibiaweb.org/images/vertnet_logo_303x85.jpg" url="http://vertnet.org/"/>
    <logo img="http://amphibiaweb.org/images/homepage_images/awlogomed.jpg" url="http://amphibiaweb.org/"/>
    <colors method="field" fieldname="darwin:scientificname" label="ScientificName">
        <color key="default" red="0" green="0" blue="255" symbol="7" label="AmphibiaWeb Species"/>
        <dominantcolor webcolor="629ad3"/>
        <subdominantcolor webcolor="ddddff"/>
    </colors>
    <settings>
        <setting name="landsat" show="0"></setting>
        <setting name="maxerrorinmeters" show="1"></setting>
        <setting name="coloruncertainty" show="1"></setting>
    </settings>
    <recordlinkback>
        <linkback method="pattern" linkurl="${char255:1}" text="View Details" fieldname="Link Back"/>
    </recordlinkback>
    <concepts xmlns:darwin="http://digir.net/schema/conceptual/darwin/2003/1.0">
        <concept order="1" viewlist="1" datatype="darwin:scientificname" alias="Scientific Name"/>
        <concept order="6" viewlist="1" datatype="darwin:decimallatitude" alias="Decimal Latitude"/>
        <concept order="7" viewlist="1" datatype="darwin:decimallongitude" alias="Decimal Longitude"/>
        <concept order="2" viewlist="1" datatype="darwin:institutioncode" alias="Institution Code"/>
        <concept order="4" viewlist="1" datatype="darwin:collectioncode" alias="Collection Code"/>
        <concept order="5" viewlist="1" datatype="darwin:catalognumber" alias="Catalog Number"/>
        <concept order="8" viewlist="0" datatype="char255:1" alias="dc_rereference"/>
    </concepts>
</berkeleymapper>

