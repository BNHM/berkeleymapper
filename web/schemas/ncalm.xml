<berkeleymapper>
	<metadata>
	 <name>NCALM Map Configuration File</name>
	 <relatedinformation>http://ncalm.berkeley.edu/</relatedinformation>
	 <abstract> BerkeleyMapper configuration file for NCALM projects map 
interface</abstract>
	 <units key="km"/>
	 <bmzoom value="13"/>
	</metadata>


<themes>
	<theme method="coloryearcollected" fieldname="darwin:yearcollected"/>
</themes>
<settings> </settings>
<colors method="dynamicfield" fieldname="char255:4" label="Project Name" /> 
<gisdata>
<layer title="Lidar Coverage Boundary" name="lawrenceville" location="http://bg.berkeley.edu/lawrenceville.lyr" legend="1" active="1" />
</gisdata>

<concepts>
	<concept order="1" viewlist="0" datatype="darwin:catalognumbertext" alias="CatalogNumber"/>	
	<concept order="9" viewlist="0" datatype="darwin:decimallongitude" alias="DecimalLongitude"/>
	<concept order="9" viewlist="0" datatype="darwin:decimallatitude" alias="DecimalLatitude"/>
	<concept order="2" viewlist="1" datatype="char120_1" alias="PI Name"/>
	<concept order="3" viewlist="1" datatype="char120_2" alias="Institution"/>
	<concept order="4" viewlist="1" datatype="char120_3" alias="State"/>
	<concept order="1" viewlist="1" datatype="char255_4" alias="Project Name"/>
	<concept order="5" viewlist="1" datatype="char120_5" alias="Data Aquired"/>
	<concept order="6" viewlist="1" datatype="char120_6" alias="Approx. coverage area (km2)"/>
	<concept order="100" viewlist="0"  datatype="darwin:yearcollected" alias="Year Collected"/>
</concepts>

</berkeleymapper>
